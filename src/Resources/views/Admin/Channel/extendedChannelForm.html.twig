    {{ form_help(form.cloudimageDevicePixelRatio) }}
    {{ form_row(form.cloudimageOrgIfSml) }}
    {{ form_help(form.cloudimageOrgIfSml) }}
    <h5 style="color: red">{{ "scaleflex_sylius_cloudimage.ui.custom"|trans }}</h5>
